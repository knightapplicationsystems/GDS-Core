Ti.UI.setBackgroundColor('Black');var tabGroup=Ti.UI.createTabGroup({barColor:'Black'});var winNews=Ti.UI.createWindow({url:'main_windows/gdsNews.js'});var tab1=Ti.UI.createTab({icon:'gds_rss.png',title:'News',window:winNews});var winAudio=Ti.UI.createWindow({url:'main_windows/gdsAudio.js'});var tab2=Ti.UI.createTab({icon:'gds_listen_2.png',title:'Shows',window:winAudio});var winTwitter=Ti.UI.createWindow({url:'main_windows/gdsTwitter.js'});var tab3=Ti.UI.createTab({icon:'gds_twitter.png',title:'Twitter',window:winTwitter});var winEmail=Ti.UI.createWindow({url:'main_windows/gdsEmail.js'});var tab4=Ti.UI.createTab({icon:'gds_email.png',title:'Email Us',window:winEmail});Ti.Media.defaultAudioSessionMode=Ti.Media.AUDIO_SESSION_MODE_PLAYBACK;tabGroup.addTab(tab1);tabGroup.addTab(tab2);tabGroup.addTab(tab3);tabGroup.addTab(tab4);tabGroup.open();